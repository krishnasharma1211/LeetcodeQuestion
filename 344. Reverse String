class Solution {
public:
    void reverseString(vector<char>& s) {
       int st =0;
       int end =s.size()-1;
       while(st<=end){
        swap(s[st],s[end]);
        st++;
        end--;
       /* int right =s.size()-left-1;
        if(left>=right)
        swap(s[left],s[right]);
        reverseString(s,left+1);
       */
}
}
};
